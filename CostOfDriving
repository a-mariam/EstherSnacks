import java.util.Scanner;

    public class CostOfDriving{
       public static void main(String [] args){
          Scanner userInput = new Scanner(System.in);
          
     System.out.println("driving distance ");
        double drivingDistance = userInput.nextDouble();
        
     System.out.println("mile per gallon ");
         double mile = userInput.nextDouble();
         
     System.out.println("price per gallon ");
          double price = userInput.nextDouble();
            double costOfDriving = (drivingDistance * price / mile);
            
     System.out.println("costOfDriving " + CostOfDriving + "$" );

}





}

  
