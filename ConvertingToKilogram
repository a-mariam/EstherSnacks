


   public class ConvertionToKilogram{
     public static void main(String[]agrs){
         
        double poundsInIbs = 55.5;
        double kilogramsInKg = 0.454;

        double ConvertionToKilogram = poundsInIbs * kilogramsInKg;

        System.out.println(ConvertionToKilogram);

}








      }
